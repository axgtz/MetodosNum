function int_simpson38(F,xi,xf,np)
    % int simp son38 v1−−−−Nombre de l a f u n c i o n
    % F  funcion matematica de entrada
    % [xi xf]−−−−Intervalo de integracion
    % np−−−−Numero de particiones
    h = (xf - xi ) / (3 * np);
    x = [xi:h:xf];
    n = size(x,2);
    Int = 0; j = 0;
    for i = 1: 3:n-1
        j = j + 1;
        I(j) = 3 / 8 * h * (F(x(i)) + 3 * F(x(i+1)) + 3 * F(x(i+2)) + F(x(i+3)));
        Int = Int + I(j);
        salida1 =['Particion' ,num2str(j) ,' ',num2str(I(j))];
        disp (salida1)
    end
    Salida2 =['IntegralTotal',' ',num2str(Int)] ;
    disp ( Salida2 )
end